<%# logger.debug('NUM: '+@num.to_s) %>
<%# logger.debug('BLOCK: '+@block.to_s) %>
<%# logger.debug('PRICE: '+@price.to_s) %>
<%# logger.debug('UNIT_NAME: '+@unit.to_s) %>
<% logger.debug("\n" + 'ARRAY: '+@prices.inspect + "\n\r") %>
var prices = <%= @prices %>
alert(prices);
$('label[class*="customer-price"]').each(function (index, element) {
  $(element).text(prices[index].toFixed(4));
  $(element).siblings('input').val(prices[index].toFixed(4));
});