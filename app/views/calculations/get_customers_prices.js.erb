<%# logger.debug('NUM: '+@num.to_s) %>
<%# logger.debug('BLOCK: '+@block.to_s) %>
<%# logger.debug('PRICE: '+@price.to_s) %>
<%# logger.debug('UNIT_NAME: '+@unit.to_s) %>
<%# logger.debug('ARRAY: '+@prices.inspect) %>
var prices = <%= @prices %>

$('input[class*="customer-price"]').each(function (index, element) {
  $(element).val(prices[index].toFixed(4));
});