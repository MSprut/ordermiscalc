<%# logger.debug('PERCENTS: ' + @percents.inspect) %>

<% @percents.each_with_index do |pr, i| %>
  $('input[id$="' + <%= i %> + '_overheads_percent"]').val(<%= pr.overheads_percent %>);
  $('input[id$="' + <%= i %> + '_manager_percent"]').val(<%= pr.manager_percent %>);
  $('input[id$="' + <%= i %> + '_profit_percent"]').val(<%= pr.profit_percent %>);
  $('input[id$="' + <%= i %> + '_tax_percent"]').val(<%= pr.tax_percent %>);
<% end %>
